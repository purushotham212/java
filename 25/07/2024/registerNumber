class Student {
    String registerNumber;
    String name;
    int marks;

    Student(String registerNumber, String name, int marks) {
        setDetails(registerNumber, name, marks);
    }

    void setDetails(String regNum, String studentName, int studentMarks) {
        registerNumber = regNum;
        name = studentName;
        marks = studentMarks;
    }

    void display() {
        System.out.println("Register Number: " + registerNumber);
        System.out.println("Name: " + name);
        System.out.println("Marks: " + marks);
    }

    public static void main(String[] args) {
        Student student = new Student("12345", "Alice", 95);
        student.display();
    }
}
