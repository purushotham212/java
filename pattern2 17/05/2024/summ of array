public class SumOfArrayElements {
    public static void main(String[] args) {
        int[] arr = { 5, 12, 9, 3, 6, 15, 7 };

        int sum = 0;
        for (int i = 0; i < arr.length; i++) {
            sum += arr[i];
        }
        
        System.out.println("Sum of Array elements: " + sum);
    }
}
